@model Rently.ViewModels.RandomMovieViewModel
@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (Model.Movies.Count == 0)
{
    <text>There is no movie available</text>
}

else
{
    <h2>Movies</h2>

    foreach (var movie in Model.Movies)
    {
        <li> @movie.Name </li>
    }
}